{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://rekor.sigstore.dev/types/participantrecord/participantrecord_schema.json",
  "title": "participantrecord schema",
  "type": "object",
  "required": ["participantId", "primaryPK"],
  "properties": {
    "participantId": {
      "type": "string",
      "description": "Unique participant ID"
    },
    "primaryPK": {
      "type": "string",
      "description": "Main public key"
    },
    "alternatePK": {
      "type": "string",
      "description": "Optional backup key"
    }
  },
  "additionalProperties": false
}
